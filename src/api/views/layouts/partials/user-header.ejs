<link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/4.7.0/css/font-awesome.min.css">


<div class="preloder_part">
  <div class="spinner">
    <div class="dot1"></div>
    <div class="dot2"></div>
  </div>
</div>
<!-- preloder end  -->

<!-- back to top start -->
<div class="progress-wrap">
  <svg
    class="progress-circle svg-content"
    width="100%"
    height="100%"
    viewBox="-1 -1 102 102"
  >
    <path d="M50,1 a49,49 0 0,1 0,98 a49,49 0 0,1 0,-98" />
  </svg>
</div>
<!-- back to top end -->

<!-- header start -->
<header
  class="header header__style-two"
  style="background-color: lightgoldenrodyellow"
>
  <div class="container mxw_1530">
    <div class="header__middle ul_li_between">
      <div class="header__logo">
        <a href="#">
          <h3 style="color: orangered"><b>E-Cart</b></h3>
        </a>
      </div>
      <div class="header-date">
        <i class="fa fa-calendar-alt"></i><%= new Date().toLocaleDateString()
        %>
      </div>
      <div class="add">
        <a href="#!">
          <img src="" alt="" />
        </a>
      </div>
      <div class="d-none d-lg-block">
        <div class="ul_li">
          <div class="header__language style-2 mr-25">
            <ul>
              <li>
                <a href="#!" class="lang-btn"
                  ><img src="" alt="" />
                  English
                  <i class="fa fa-chevron-down"></i
                ></a>
                <ul class="lang_sub_list">
                  <li><a href="#">English</a></li>
                </ul>
              </li>
            </ul>
          </div>
          <div class="header__social">
            <a href="#!"><i class="fa fa-facebook-f"></i></a>
            <a href="#!"><i class="fa fa-twitter"></i></a>
            <a href="#!"><i class="fa fa-instagram"></i></a>
            <a href="#!"><i class="fa fa-youtube"></i></a>
          </div>
        </div>
      </div>
    </div>
  </div>
  <div
    class="header__wrap"
    data-uk-sticky="top: 250; animation: uk-animation-slide-top;"
  >
    <div class="container mxw_1530">
      <div class="header__main ul_li">
        <div class="header__logo">
          <a href="#">
            <img src="" alt="" />
          </a>
        </div>

        <div class="hamburger_menu d-lg-none">
          <a href="javascript:void(0);" class="active">
            <div class="icon bar">
              <span><i class="fa fa-bars"></i></span>
            </div>
          </a>
        </div>
        <div class="main-menu navbar navbar-expand-lg">
          <nav class="main-menu__nav collapse navbar-collapse">
            <ul>
              <li class=" has-mega-menu active">
                <a href="/users/dashboard">Home</a>
              </li>
             
             

              <li><a href="/users/all-products/6515c19eb6b85670909a53bf">Smart Phones</a></li>

              <li><a href="/users/all-products/651456f4e8a5b44f4dda6f7f">Tablets</a></li>
              <li><a href="/users/all-products/6514570be8a5b44f4dda6f83">Laptops</a></li>
              <li><a href="/users/all-products/6515c1c5b6b85670909a53c3">Head Phones</a></li>

              <li><a href="/users/all-products/65145729e8a5b44f4dda6f87">Accessories</a></li>
            </ul>
          </nav>
        </div>
        <div class="header__main-right ul_li">
          <div class="header__icons ul_li mr-15">
            <div class="icon">
              <a href="/users/wishlist" class="cart-link">
                <i class="fa fa-heart" style="font-size:20px"></i>
                <% if(user){ %>
                  <span class="count" style="position: absolute; top: -5px; right: -5px; background-color: orange; color: white; border-radius: 50%; padding: 3px 5px; font-size: 12px;"><%- wishlistCount %></span>

                <% } %>
              </a>
            </div>

            <div class="icon">
              <a href="/users/cart" class="cart-link">
                  
                  <i class="fa fa-shopping-cart" style="font-size:24px"></i>
                  <% if(user){ %>

                    <span class="count" style="position: absolute; top: -5px; right: -5px; background-color: orange; color: white; border-radius: 50%; padding: 3px 5px; font-size: 12px;"><%- cartCount %></span>

                  <% } %>
              </a>
          </div>
          
          </div>
          <% if(user){ %>
          <div class="icon dropdown">
            <a
              href="#"
              class="user-link"
              id="userDropdown"
              role="button"
              data-bs-toggle="dropdown"
              aria-expanded="false"
            >
              <i class="fa fa-user" style="font-size: 24px;"></i>
            </a>
            <ul class="dropdown-menu" aria-labelledby="userDropdown">
              <!-- Add your profile information and customization here -->
              <li>
                <a class="dropdown-item" href="/users/accounts">Profile</a>
              </li>
             
              <li><hr class="dropdown-divider" /></li>
              <li><a class="dropdown-item" href="/users/logout">Logout</a></li>
            </ul>
          </div>
          <% }else{ %>
          <div class="login-sign-btn">
            <a class="thm-btn thm-btn__2 text-black" href="/users/login">
              <span class="btn-wrap">
                <span>Login / Sign Up</span>
                <span>Login / Sign Up</span>
              </span>
            </a>
          </div>
          <% } %>
        </div>
      </div>
    </div>
  </div>
</header>
